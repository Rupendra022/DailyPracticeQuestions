class Solution {
    // RADHE RADHE 
    public void postOrder(Node root ,  ArrayList<Integer> ans){
        if(root==null) return ; 
        
        postOrder(root.left,ans);
        postOrder(root.right,ans);
        ans.add(root.data);
    }
    public ArrayList<Integer> postOrder(Node root) {
        // code here
         ArrayList<Integer> ans = new ArrayList<>();
        postOrder(root,ans);
        
        return ans ;
    }
}
